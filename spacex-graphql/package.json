{
	"name": "spacex-graphql",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@reduxjs/toolkit": "^1.5.1",
		"apollo-boost": "^0.4.9",
		"axios": "^0.21.1",
		"framer-motion": "^4.1.17",
		"graphql": "^15.5.0",
		"react": "^17.0.2",
		"react-apollo-hooks": "^0.5.0",
		"react-dom": "^17.0.2",
		"react-fullscreen-image": "0.0.3",
		"react-icons": "^4.2.0",
		"react-loading-skeleton": "^2.2.0",
		"react-redux": "^7.2.4",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"redux": "^4.1.0",
		"styled-components": "^5.3.0",
		"typescript": "^4.2.4"
	},
	"scripts": {
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test --coverage --watchAll",
		"eject": "react-scripts eject",
		"generate": "graphql-codegen --config codegen.js"
	},
	"eslintConfig": {
		"extends": "react-app"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"jest": {
		"collectCoverageFrom": [
			"<rootDir>/src/**/*.tsx",
			"<rootDir>/src/**/*.ts",
			"!<rootDir>/src/App.tsx",
			"!<rootDir>/src/index.tsx",
			"!<rootDir>/src/utils/index.ts",
			"!<rootDir>/src/pages/index.ts",
			"!<rootDir>/src/components/index.ts",
			"!<rootDir>/src/hooks/*.ts",
			"!<rootDir>/src/queries/*.ts",
			"!<rootDir>/src/models/*.ts"
		],
		"coverageThreshold": {
			"global": {
				"branches": 90,
				"functions": 90,
				"lines": 90,
				"statements": 90
			}
		},
		"coverageReporters": [
			"html",
			"text"
		]
	},
	"devDependencies": {
		"@graphql-codegen/cli": "^1.21.4",
		"@testing-library/jest-dom": "^5.12.0",
		"@testing-library/react": "^11.2.7",
		"@testing-library/user-event": "^13.1.9",
		"@types/jest": "^26.0.23",
		"@types/node": "^15.6.1",
		"@types/react": "^17.0.7",
		"@types/react-dom": "^17.0.5",
		"@types/react-router-dom": "^5.1.7",
		"@types/redux": "^3.6.0",
		"@types/styled-components": "^5.1.9"
	}
}
